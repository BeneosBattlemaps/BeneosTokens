ue":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[["2d6","slashing"]],"versatile":"7"},"formula":"","save":{"ability":"str","dc":null,"scaling":"str"},"requirements":"","recharge":{"value":null,"charged":false}},"sort":400000,"flags":{},"img":"beneos_tokens_assets//_ability_icons/161.webp","effects":[],"folder":null,"permission":{"default":0}},{"_id":"HS2Ac9MPJTaBmLqR","name":"Porcine Frenzy","type":"feat","data":{"description":{"value":"<p><strong>Porcine Frenzy.</strong>&nbsp;When the Wereboar is reduced to 0 hit points, it can drop to 1 hit point instead. It can't use this feature again until the next day. However, each time the Wereboar starts its turn with 1 hit point, it gains 7 (2d6) temporary hit points at the start of its turn and can make an additional Gore attack instead of a Haymaker.</p>","chat":"","unidentified":""},"source":"Beneos Tokens","activation":{"type":"special","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"self","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"heal","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[["2d6","temphp"]],"versatile":"7"},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":null,"charged":false}},"sort":1200000,"flags":{},"img":"beneos_tokens_assets//_ability_icons/skill_65.webp","effects":[],"folder":null,"permission":{"default":0}}],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"7ptJY7RGU63YcaB9":3},"flags":{"core":{"sourceId":"Actor.EzhbnVaPO6midRkC"},"scene-packer":{"sourceId":"Actor.","hash":"d40c31286d4e002ba36e652ad9fa0606d965a